            "host": "^Enabled$",
            "os_ping": "^1$",
            "os_login": "^1$",
            "os_run_cmd": "^1$"
        },
        "bmc_reboot": 0,
        "method_type": "keyword",
        "method": "Run External IPMI Standard Command  power on -N 10 -R 1",
        "lib_file_path": "ipmi_client.robot"
    },

