            "host": "^Enabled$",
            "os_ping": "^1$",
            "os_login": "^1$",
            "os_run_cmd": "^1$"
        },
        "bmc_reboot": 1,
        "method_type": "keyword",
        "method": "Set Global Variable  ${PDU_TYPE}  pdu ; Set Global Variable  ${PDU_IP}  ${PDU_HOST} ; PDU Power Cycle",
        "lib_file_path": "pdu/pdu.robot:pdu/synaccess.robot"
    },
